  <!-- Brand Theme (GAYATRI) -->
  <style>
    :root{
      --brand:#c4202d;          /* primary from logo */
      --brand-700:#981923;      /* hover */
      --brand-800:#6c1219;      /* active/pressed */
      --brand-300:#e6646f;      /* light accents (dots/badges) */
      --text:#0f172a;
      --bg:#f8fafc;
    }

    /* Logo font */
    .logo_roman{
      font-family:"Libre Baskerville", Georgia, "Times New Roman", serif;
      font-weight:700;
      text-transform:uppercase;
      letter-spacing:-0.015em;
      color:var(--brand);
      line-height:1;
      margin:0;
    }

    /* Base */
    body{ color:var(--text); background:var(--bg); }
    a, .link-primary{ color:var(--brand); }
    a:hover, .link-primary:hover{ color:var(--brand-700); }

    /* Topbar */
    .topbar{ background:#0b1223; }
    /*.topbar {background:#657179}*/
    /* Navbar */
    /*.navbar{ background:var(--brand); }*/
    .navbar .navbar-brand, .navbar .nav-link, .navbar .navbar-brand h1{ color:#fff !important; }


    .navbar .nav-link:hover, .navbar .nav-link.active{ opacity:.9; color:#fff !important; }

    /* Buttons */
    .btn-primary{
      color:#fff !important;
      background:var(--brand) !important;
      border-color:var(--brand) !important;
    }
    .btn-primary:hover, .btn-primary:focus{
      background:var(--brand-700) !important;
      border-color:var(--brand-700) !important;
    }
    .btn-outline-light{
      border-color:#fff !important; color:#fff !important;
    }
    .btn-outline-light:hover{ background:#fff !important; color:var(--brand) !important; }
    .btn-outline-primary{
      color:var(--brand) !important; border-color:var(--brand) !important;
    }
    .btn-outline-primary:hover{
      color:#fff !important; background:var(--brand) !important; border-color:var(--brand) !important;
    }

    /* Badges/labels */
    .badge-primary, .label-primary{ background:var(--brand); color:#fff; }

    /* Carousel controls */
    .carousel-control-prev-icon,
    .carousel-control-next-icon{
      filter: drop-shadow(0 2px 8px rgba(0,0,0,.35));
    }
    .carousel-caption h5{ text-shadow: 0 2px 8px rgba(0,0,0,.45); }
    .carousel-caption h1{ text-shadow: 0 3px 12px rgba(0,0,0,.5); }

    /* Sections */
    .section-title h5.text-primary{ color:var(--brand) !important; }

    /* Service cards */
    .service-item .service-icon{
      width: 64px; height: 64px; border-radius: 12px;
      background:var(--brand); display:flex; align-items:center; justify-content:center;
      box-shadow:0 6px 18px rgba(196,32,45,.25);
      margin-top:-32px;
    }

    /* Facts blocks */
    .facts .bg-primary{ background:var(--brand) !important; }
    .facts .bg-primary .text-primary{ color:var(--brand) !important; }

    /* Feature dots (owl) */
    .owl-theme .owl-dots .owl-dot span{ background:var(--brand-300); }
    .owl-theme .owl-dots .owl-dot.active span{ background:var(--brand); }

    /* Team */
    .team-item .team-social a{ background:var(--brand); border-color:var(--brand); }
    .team-item .team-social a:hover{ background:var(--brand-700); border-color:var(--brand-700); }

    /* Blog labels */
    .blog-img a.bg-primary{ background:var(--brand) !important; }

    /* Footer */
    .footer-about .bg-primary{ background:var(--brand) !important; }
    .btn.btn-primary.btn-square{ background:var(--brand); border-color:var(--brand); }
    .btn.btn-primary.btn-square:hover{ background:var(--brand-700); border-color:var(--brand-700); }

    /* Inputs in brand panels */
    .bg-primary .form-control,
    .bg-primary .form-select,
    .bg-primary textarea{
      background:rgba(255,255,255,.95) !important;
      border:0 !important;
    }

    /* Spinner */
    #spinner .spinner{
      width:3rem;height:3rem;border:.35rem solid #eee;border-top-color:var(--brand);
      border-radius:50%; animation:spin 1s linear infinite;
    }
    @keyframes spin{ to{ transform: rotate(360deg); } }
  </style>
<style>
/* Navbar scroll fix */
.navbar{ transition: background-color .2s ease, box-shadow .2s ease; }

/* When scrolled we flip to white background and dark text */
.navbar.scrolled,
.navbar.nav-sticky,
.sticky-top.navbar {
  background:#fff !important;
/*background: #657179  !important;*/

  box-shadow:0 6px 18px rgba(0,0,0,.08);
