/* Make links visible on white */
.navbar.scrolled .nav-link,
.navbar.nav-sticky .nav-link,
.sticky-top.navbar .nav-link {
  color:#0f172a !important;                 /* dark text */
}

/* Hover/active in brand color */
.navbar.scrolled .nav-link:hover,
.navbar.scrolled .nav-link.active,
.navbar.nav-sticky .nav-link:hover,
.navbar.nav-sticky .nav-link.active,
.sticky-top.navbar .nav-link:hover,
.sticky-top.navbar .nav-link.active {
  color:#c4202d !important;                 /* brand red */
}

/* Brand + icons readable on white */
.navbar.scrolled .navbar-brand,
.navbar.scrolled .navbar-brand h1,
.navbar.nav-sticky .navbar-brand,
.sticky-top.navbar .navbar-brand {
  color:#c4202d !important;
}
.navbar.scrolled .btn.text-white,
.navbar.nav-sticky .btn.text-white,
.sticky-top.navbar .btn.text-white {
  color:#0f172a !important;                 /* search icon button */
}
.navbar.scrolled .navbar-toggler,
.navbar.nav-sticky .navbar-toggler,
.sticky-top.navbar .navbar-toggler {
  border-color:#0f172a;
}
.navbar.scrolled .navbar-toggler .fa-bars,
.navbar.nav-sticky .navbar-toggler .fa-bars,
.sticky-top.navbar .navbar-toggler .fa-bars {
  color:#0f172a;
}
</style>


</head>

